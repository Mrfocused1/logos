rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /testimonials/{allPaths=**} {
      allow read: if true; // Public read
      allow write: if request.auth != null
        && request.resource.size < 5 * 1024 * 1024 // 5MB max
        && request.resource.contentType.matches('image/.*');
    }
  }
}